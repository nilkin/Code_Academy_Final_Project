@model Breadcrumb
<nav class="breadcrumb-section">
    <img src="~/uploads/@Model.Photo" class="">
    <div style="
    position: relative;
    bottom: 16rem;
">
        <div class="breadcrumb-title text-center my-20">
            <h2 class="title text-dark text-capitalize">@Model.Title</h2>
        </div>

        <ol class="breadcrumb bg-transparent m-0 p-0 align-items-center justify-content-center">
            <li class="breadcrumb-item"><a href="index.html"> @Model.Path</a></li>
            <li class="breadcrumb-item active" aria-current="page">
                <i class="fas fa-angle-right"></i> @Model.Link
            </li>
        </ol>
    </div>
  

</nav>

